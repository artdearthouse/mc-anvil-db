# HopperMC Environment Configuration

# Database connection string
DATABASE_URL=postgres://postgres:postgres@db:5432/hoppermc

POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=hoppermc

# World generator: "flat" or "vanilla"
GENERATOR=flat

# World seed (for vanilla generator)
SEED=0

# Storage mode: "nostorage" (stateless) or "raw" (PostgreSQL)
STORAGE=raw

RUST_LOG=info

MC_DATA_VERSION=4671

BENCHMARK=true